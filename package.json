{
  "name": "piano-hero",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "author": "Alexandre Oger <oger.alexandre@gmail.com>",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "vite",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "build": "pnpm format && pnpm test && vite build",
    "build:watch": "tsc && vite build --watch",
    "lint": "eslint src && pnpm prettier . --check && oxlint -A all",
    "format": "eslint src --fix && pnpm prettier . --write && oxlint -A all --fix",
    "test": "vitest --run --typecheck",
    "test:watch": "vitest --typecheck",
    "test:ui": "vitest --ui --typecheck",
    "coverage": "vitest run --coverage",
    "knip": "knip"
  },
  "dependencies": {
    "effect": "2.0.0-next.59",
    "list": "2.0.19",
    "shades": "2.2.0"
  },
  "devDependencies": {
    "@esbuild/darwin-arm64": "0.19.10",
    "@sveltejs/vite-plugin-svelte": "3.0.1",
    "@tsconfig/svelte": "5.0.2",
    "@typescript-eslint/eslint-plugin": "6.15.0",
    "@typescript-eslint/parser": "6.15.0",
    "@vitest/coverage-v8": "1.0.4",
    "@vitest/ui": "1.0.4",
    "esbuild": "0.19.10",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-functional": "6.0.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-svelte": "2.35.1",
    "fast-check": "3.15.0",
    "knip": "3.8.3",
    "oxlint": "0.0.20",
    "prettier": "3.1.1",
    "prettier-plugin-svelte": "3.1.2",
    "svelte": "4.2.8",
    "svelte-check": "3.6.2",
    "ts-essentials": "9.4.1",
    "tslib": "2.6.2",
    "typescript": "5.3.3",
    "vite": "5.0.10",
    "vitest": "1.0.4"
  }
}
